      "parameters": {
        "functionCode": "\n//07 DE MAYO DEL 2022\nconst getMonthNumber = (monthText) =>{\nif(monthText === 'enero') return '01';\nif(monthText === 'febrero') return '02';\nif(monthText === 'marzo') return '03';\nif(monthText === 'abril') return '04';\nif(monthText === 'mayo') return '05';\nif(monthText === 'junio') return '06';\nif(monthText === 'julio') return '07';\nif(monthText === 'agosto') return '08';\nif(monthText === 'septiembre') return '09';\nif(monthText === 'octubre') return '10';\nif(monthText === 'noviembre') return '11';\nif(monthText === 'diciembre') monthNumber = '12';\n};\nconst [dayText,,monthTextString,,yearText] = item.date.split(' ');\nconst monthText = monthTextString.replace(/\\s+/g, '').trim().toLowerCase();\nlet monthNumber = getMonthNumber(monthText);\n\nconst newDate = `${dayText.trim()}-${monthNumber}-${yearText.trim()}`;\nreturn {\n  rss_item: \n`<item>\n  <title>${item.title}</title>\n  <link>${item.link}</link>\n  <description>${item.description}</description>\n  <pubDate>${newDate}</pubDate>\n</item>`\n}"
      },
